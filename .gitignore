# Node modules (if you ever add JS tooling)
node_modules/

# Python virtual environments (just in case)
venv/
.env/

# Ignore Docker volumes (if bind mounts accidentally created anything)
*.pid
*.sock
*.log
*.db

# Ignore any local config overrides
litellm/config.local.yaml
litellm/.env
open-webui/config.local.yaml
open-webui/.env

# Ignore Docker Compose overrides
docker-compose.override.yml

# Ignore system files
.DS_Store
Thumbs.db

# Ignore vscode or other editor settings
.vscode/
.idea/
*.swp

# Ignore build artifacts
dist/
build/
tmp/
.cache/

# Ignore backups or temp files
*.bak
*.tmp
*.old

# Ignore generated files
*.out

# Optional: if you add a "scripts" folder
scripts/*.log
scripts/*.tmp

# LiteLLM temp stuff (if you configure logging later)
litellm/logs/

# Ignore everything inside persistent-data
persistent-data/*

# But track the directory itself
!persistent-data/
!persistent-data/README.md

.idea
.vscode

.env

# --- Terraform (scoped to infra/tf) ---
# Local working dirs (providers/plugins cache)
infra/tf/**/.terraform/

# State files (keep these out of git; store remotely like S3 instead)
infra/tf/**/*.tfstate
infra/tf/**/*.tfstate.*

# Crash logs
infra/tf/**/crash.log
infra/tf/**/crash.*.log

# Sensitive variable files (use *.example for committed templates)
infra/tf/**/*.tfvars
infra/tf/**/*.tfvars.json
infra/tf/**/terraform.tfvars
infra/tf/**/terraform.tfvars.json
infra/tf/**/*.auto.tfvars
infra/tf/**/*.auto.tfvars.json

# Local plans / outputs produced by CLI
infra/tf/**/*.tfplan
infra/tf/**/*.plan
infra/tf/**/*.out

# Editor/OS noise (optional, but helpful)
infra/tf/**/.DS_Store
infra/tf/**/.idea/
infra/tf/**/.vscode/
infra/tf/tfplan
